



<div class="category">
    <div class="category__title text center">{{ category }}</div>

    <div class="category__content">
        {% for book in paginator %}
        <div>
            <img class="category__content__img" src="{{ book.imageLink }}{{ book.bookId }}.png">
            <div>
                {{ book.title|slice(0,17) }}...
            </div>

        </div>
        {% endfor %}
    </div>

</div>
